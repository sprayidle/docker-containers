<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Direno</Name>
  <Description>
    Direno[br]
    [b][span style='color: #E80000;']Directions:[/span][/b][br]
    Set container port for webui (5000).[br]
    [b]/config[/b] : Where direno will store its configuration files.[br]
  </Description>
  <Registry>https://registry.hub.docker.com/u/sprayidle/direno/</Registry>
  <GitHub>https://github.com/sprayidle/docker-dirneo</GitHub>
  <Repository>sprayidle/dirneo</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5000</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/direno</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:5000]/</WebUI>
  <Icon>https://i.imgur.com/bY3YIlw.png</Icon>
</Container>
